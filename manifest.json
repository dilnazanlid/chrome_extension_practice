{
	"manifest_version": 2,
	"name": "First Extension",
	"version": 1.0,
	"content_scripts": [
		{
			"matches": [
				"http://coursera.org/*",
				"https://coursera.org/*"
			],
			"js": ["content.js"]
		}
	], 
	"background":{
		"scripts": ["background.js"]
	}, 
	"browser_action": {
		"default_icon": "icon.png"
	}
}
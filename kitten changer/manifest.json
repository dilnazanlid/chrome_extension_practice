{
	"manifest_version": 2,
	"name": "Kitten Picture Extension",
	"version": "0.1",
	"web_accessible_resources":[
		"*.jpeg"
	],
	"content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
	"browser_action": {
    "default_icon": "icon.png"
  }
}

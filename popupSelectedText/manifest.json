{
	"manifest_version": 2,
	"name": "Word Description Extension",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"background":{
		"scripts": ["background.js"]
	},
	"browser_action": {
    "default_icon": "icon.png",
		"default_popup":"popup.html",
		"default_title":"Word Definition Show"
  }
}
